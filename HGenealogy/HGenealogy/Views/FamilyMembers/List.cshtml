@model IEnumerable<HGenealogy.Data.FamilyMember>

@{
    ViewBag.Title = "List";
}

<h4>List of Members</h4>

<hr />

@{ ViewData["SelectedPedigreeSecondMenu"] = "MemberList";}

<div class="pedigreecontainer container">
    <ul class="nav nav-tabs">
        <li class="active"><a href="#pedigreecontent">族譜名稱：謝氏族譜(全譜)</a></li>
    </ul>

    <div id="pedigreecontent" class="container">
        @Html.Partial("_PedigreeSecondMenu", @ViewData)
    </div>

    <h3 class="text-center">CONTACT</h3>

    <!--成員列表-->
    @foreach (var item in Model)
    {
        <!-- member Section) -->
        <div id="member" class="container-fluid bg-grey">           
            <div class="row">
                <div class="col-sm-12">
                    <p>姓名：</p>
                    <p><span class="glyphicon glyphicon-map-marker"></span>住址....</p>
                    <p><span class="glyphicon glyphicon-phone"></span> +00 1515151515</p>
                    <p><span class="glyphicon glyphicon-envelope"></span> myemail@something.com</p>
                </div>
            </div>
        </div>
    }

</div>

